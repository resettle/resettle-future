{
  "name": "@services/intelligence",
  "private": true,
  "type": "module",
  "scripts": {
    "typegen": "dotenvx-cloudflare run -f ../../.env -e 'DOTENV_|VITE_' -- wrangler types --config wrangler.toml",
    "typegen:staging": "dotenvx-cloudflare run -f ../../.env.staging -e 'DOTENV_|VITE_' -- wrangler types --config wrangler.staging.toml",
    "typegen:production": "dotenvx-cloudflare run -f ../../.env.production -e 'DOTENV_|VITE_' -- wrangler types --config wrangler.production.toml",
    "dev": "dotenvx-cloudflare run -f ../../.env -e 'DOTENV_|VITE_' -- wrangler dev --config wrangler.toml --port 3001 --inspector-port 9241",
    "deploy:staging": "dotenvx run -f ../../.env.staging -- wrangler deploy --config wrangler.staging.toml",
    "deploy:production": "dotenvx run -f ../../.env.production -- wrangler deploy --config wrangler.production.toml",
    "secrets:staging": "dotenvx-cloudflare sync -f ../../.env.staging -e 'DOTENV_|VITE_' -n resettle-staging-service-intelligence",
    "secrets:production": "dotenvx-cloudflare sync -f ../../.env.production -e 'DOTENV_|VITE_' -n resettle-production-service-intelligence",
    "kysely": "dotenvx run -f ../../.env --overload -- kysely",
    "kysely:staging": "dotenvx run -f ../../.env.staging --overload -- kysely",
    "kysely:production": "dotenvx run -f ../../.env.production --overload -- kysely",
    "test:integration": "dotenvx run -f ../../.env --overload -- tsx --test --test-isolation='none' --experimental-test-module-mocks src/**/*.spec.ts"
  },
  "dependencies": {
    "@resettle/api": "workspace:^",
    "@resettle/database": "workspace:^",
    "@resettle/schema": "workspace:^",
    "@resettle/utils": "workspace:^",
    "@services/_common": "workspace:^",
    "date-fns": "4.1.0",
    "hono": "4.10.8",
    "hono-openapi": "1.1.2",
    "kysely": "0.28.8",
    "openai": "6.10.0",
    "pg": "8.16.3",
    "pgvector": "0.2.1",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@types/node": "25.0.1",
    "@types/pg": "8.16.0",
    "kysely-ctl": "0.19.0",
    "typescript": "5.9.3"
  }
}
